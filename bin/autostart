#! /usr/bin/env bash

killall -q dunst
while pgrep -u $UID -x dunst >/dev/null; do sleep 1; done
dunst &

killall -q xidlehook
while pgrep -u $UID -x xidlehook >/dev/null; do sleep 1; done
~/bin/idle -s & 

~/.config/polybar/scripts/launch-polybar.sh specbar &
~/usr/lib/polkit-kde-authentication-agent-1 &
/usr/lib/kdeconnectd &
picom --experimental-backends -b &
hsetroot -cover ~/Pictures/Wallpapers/black5_unicat.png &
pgrep -x copyq >/dev/null || copyq &
pgrep -x nm-applet >/dev/null || nm-applet &
pgrep -x udiskie >/dev/null || udiskie -t &
/bin/disableETPS 0 &
