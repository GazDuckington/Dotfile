for_window [app_id="dropdown-terminal"] move scratchpad, scratchpad show

bindsym Mod4+F2 exec swaymsg [app_id='dropdown-terminal'] scratchpad show || exec kitty --class dropdown-terminal

for_window [app_id="musikcube"] move scratchpad, scratchpad show
bindsym Mod4+F3 exec swaymsg [app_id="musikcube"] scratchpad show || \
	exec footclient -a musikcube musikcube

bindsym Mod4+F1 scratchpad show
bindsym Mod4+Shift+F1 move scratchpad

