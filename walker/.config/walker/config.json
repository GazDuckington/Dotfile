{
  "activation_mode": {
    "labels": "jkl;asdf"
  },
  "as_window": false,
  "builtins": {
    "applications": {
      "context_aware": true,
      "name": "applications",
      "placeholder": "Applications",
      "prioritize_new": true,
      "refresh": true,
      "show_generic": true,
      "show_icon_when_single": true,
      "show_sub_when_single": true,
      "weight": 5,
			"actions": {
				"enabled": true
			}
    },
    "calc": {
      "icon": "accessories-calculator",
      "min_chars": 1,
      "name": "calc",
      "placeholder": "󰃬 Calculator",
			"prefix": "=",
      "weight": 5
    },
    "clipboard": {
      "avoid_line_breaks": true,
      "exec": "wl-copy",
      "image_height": 300,
      "max_entries": 10,
      "name": "clipboard",
      "placeholder": " Clipboard",
      "switcher_only": true,
      "weight": 5
    },
    "commands": {
      "icon": "utilities-terminal",
      "name": "commands",
      "placeholder": "  Commands",
      "switcher_only": true,
      "weight": 5
    },
    "custom_commands": {
      "icon": "utilities-terminal",
      "name": "custom_commands",
      "placeholder": "Custom Commands",
      "weight": 5
    },
    "dmenu": {
      "name": "dmenu",
      "placeholder": "Dmenu",
      "switcher_only": true,
      "weight": 5
    },
    "emojis": {
      "history": true,
      "name": "emojis",
      "placeholder": " Emojis",
      "show_unqualified": false,
      "switcher_only": true,
      "typeahead": true,
      "weight": 5
    },
    "finder": {
      "concurrency": 8,
      "icon": "folder",
      "ignore_gitignore": true,
      "name": "finder",
      "placeholder": " Finder",
			"prefix": "//",
      "refresh": true,
      "switcher_only": true,
      "weight": 5
    },
    "runner": {
      "generic_entry": true,
			"shell_config": "~/.config/fish/config.fish",
      "history": true,
      "icon": "utilities-terminal",
      "name": "runner",
      "placeholder": " Runner",
			"prefix": ">",
      "refresh": true,
      "typeahead": true,
      "weight": 5
    },
    "ssh": {
      "history": true,
      "icon": "preferences-system-network",
      "name": "ssh",
      "placeholder": "SSH",
      "refresh": true,
      "switcher_only": true,
      "weight": 5
    },
    "switcher": {
      "name": "switcher",
      "placeholder": "Switcher",
      "prefix": "/",
      "weight": 5
    },
    "websearch": {
      "engines": [
        "google"
      ],
      "icon": "applications-internet",
      "name": "websearch",
      "placeholder": "󰜏 Websearch",
			"prefix": "?",
      "weight": 5
    },
    "windows": {
      "icon": "view-restore",
      "name": "windows",
      "placeholder": "󰖲 Windows",
			"prefix": "w ",
      "weight": 5
    }
	},
  "disable_click_to_close": false,
  "force_keyboard_focus": false,
  "list": {
    "max_entries": 50,
    "placeholder": "No Results",
    "show_initial_entries": true,
    "single_click": true,
    "visibility_threshold": 20
  },
  "monitor": "",
  "search": {
    "delay": 0,
    "placeholder": "Search..."
  },
  "theme": "catppuccin",
  "timeout": 0,
	"plugins":[
		{
			"name": "power",
			"placeholder": "⏼  Power",
			"prefix": "p ",
			"switcher_only": true,
			"recalculate_score": true,
			"show_icon_when_single": true,
			"entries": [
        {
          "label": "Log Out",
          "icon": "system-log-out",
          "exec": "loginctl terminate-user $(whoami)"
        },
				{
					"label": "Shutdown",
					"icon": "system-shutdown",
					"exec": "shutdown now"
				},
				{
					"label": "Reboot",
					"icon": "system-reboot",
					"exec": "reboot"
				},
				{
					"label": "Lock Screen",
					"icon": "system-lock-screen",
					"exec": "playerctl --all-players pause & hyprlock"
				}
			]
		},
		{
			"name": "emotes",
			"prefix": "e ",
			"placeholder": "  Emoticons ",
			"switcher_only": true,
			"recalculate_score": true,
			"show_icon_when_single": true,
			"src_once": "cat ~/.config/emotes/data/*",
			"cmd": "echo \"%RESULT%\" | awk -F':' '{print $1}' | tr -d '\n' | wl-copy  && notify-send -t 800 \"Symbol Copied\n\" \"%RESULT%\""
		},
		{
			"name": "scrot",
			"placeholder": "  Screen Shot ",
			"prefix": "b ",
			"switcher_only": true,
			"recalculate_score": true,
			"show_icon_when_single": true,
			"entries": [
				{
					"label": "󰆟  Area",
					"exec": "~/.config/walker/scripts/scrotting.sh area"
				},
				{
					"label": "󰹑  Screen",
					"exec": "~/.config/walker/scripts/scrotting.sh screen"
				},
				{
					"label": "  All Output(s)",
					"exec": "~/.config/walker/scripts/scrotting.sh all"
				}
			]
		}
	]
}
