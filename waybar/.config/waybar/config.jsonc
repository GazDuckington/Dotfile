{
	"ipc": true,
	"layer": "bottom",
	"position": "top",
	"margin-left": 10,
	"margin-bottom": 0,
	"margin-right": 10,
	"spacing": 5,
	"modules-left": [
		"sway/mode",
		"hyprland/workspaces",
		"sway/workspaces",
		"temperature",
		"idle_inhibitor",
		"hyprland/window",
		"sway/window",
	],
	"modules-center": [],
	"modules-right": [
		"clock#date",
		// "custom/storage",
		// "memory",
		// "cpu",
		"battery",
		"backlight",
		"wireplumber",
		// "keyboard-state",
		"custom/caps",
		"custom/power",
		"tray",
		"custom/pacman",
		"custom/notification"
	],
	"sway/window": {
		"format": "{title}",
		"max-length": 50,
		"icon": true,
		"icon-size": 10,
	},
	"custom/sp1": {
		"format": " | ",
		"tooltip": false
	},
	"custom/sp2": {
		"format": " |",
		"tooltip": false
	},
	"custom/rofi": {
		"format": "ÔÉâ",
		"tooltip": false,
		"on-click-right": "nwg-drawer",
		"on-click": "wofi --show run",
		"on-click-middle": "pkill -9 wofi"
	},
	"custom/screenshot_t": {
		"format": "ÔÄ∞",
		"on-click": "~/.config/hypr/scripts/screenshot_full",
		"on-click-right": "~/.config/hypr/scripts/screenshot_area"
	},
	"clock#1": {
		"format": "ÔÅ≥ {:%a}",
		"tooltip": false,
		"on-click": "gsimplecal"
	},
	"clock#2": {
		"format": "Ôâ¥ {:%d-%h-%Y}",
		"tooltip": false,
		"on-click": "gsimplecal"
	},
	"clock#3": {
		"format": "ÔÄó {:%H:%M:%S %p}",
		"tooltip": false,
		"on-click": "gsimplecal"
	},
	"temperature": {
		"interval": 4,
		"critical-threshold": 80,
		"format-critical": "ÔÅ±  {temperatureC}¬∞C",
		"format": "{icon}  {temperatureC}¬∞C",
		"format-icons": [
			"Ôãã",
			"Ôãâ",
			"Û∞∏Å"
		],
		"max-length": 7,
		"min-length": 7,
		"on-click": "xsensors"
	},
	"memory": {
		"interval": 30,
		"format": "Ôãõ  {used:0.2f} / {total:0.0f} GB",
		"on-click": "~/.local/bin/footclient-launch.sh btop"
	},
	"battery": {
		"interval": 2,
		"states": {
			"good": 95,
			"warning": 30,
			"critical": 15
		},
		"format": "{icon}  {capacity}%",
		"format-charging": "Û±ä¶ {capacity}%",
		"format-plugged": "Ôá¶ {capacity}%",
		"format-icons": [
			"ÔâÑ",
			"ÔâÉ",
			"ÔâÇ",
			"ÔâÅ",
			"ÔâÄ"
		]
	},
	"network": {
		"format-wifi": "Ôá´ {essid} ({signalStrength}%)",
		"format-ethernet": "{ifname}: {ipaddr}/{cidr} Ôûñ",
		"format-linked": "{ifname} (No IP) Ôûñ",
		"format": "ÔÄí",
		"format-disconnected": "Ôöî",
		"format-alt": "{ifname}: {ipaddr}/{cidr}",
		"on-click": "wl-copy $(ip address show up scope global | grep inet | head -n1 | cut -d/ -f 1 | tr -d [:space:] | cut -c5-)",
		"on-click-right": "wl-copy $(ip address show up scope global | grep inet6 | head -n1 | cut -d/ -f 1 | tr -d [:space:] | cut -c6-)",
		"tooltip-format": "ÔÇì {bandwidthUpBits} ÔÄô {bandwidthDownBits}\n{ifname}\n{ipaddr}/{cidr}\n",
		"tooltip-format-wifi": "Ôá´ {essid} {frequency}MHz\nStrength: {signaldBm}dBm ({signalStrength}%)\nIP: {ipaddr}/{cidr}\nÔÇì {bandwidthUpBits} ÔÄô {bandwidthDownBits}",
		"interval": 10
	},
	"custom/storage": {
		"format": "ÔÇ† {}",
		"format-alt": "{percentage}% ÔÇ†",
		"format-alt-click": "click-right",
		"return-type": "json",
		"interval": 60,
		"exec": "~/.config/waybar/modules/storage.sh"
	},
	"backlight": {
		"device": "intel_backlight",
		"format": "{icon} {percent}%",
		"format-alt": "{percent}% {icon}",
		"format-alt-click": "click-right",
		"format-icons": [
			"ÔÜÖ",
			"ÔÑë"
		],
		"on-scroll-up": "~/.local/bin/changebrightness.sh up 5",
		"on-scroll-down": "~/.local/bin/changebrightness.sh down 5",
	},
	"idle_inhibitor": {
		"format": "{icon}",
		"format-icons": {
			"activated": "Û∞Ö∂",
			"deactivated": "Û∞õä"
		},
		"tooltip": "true"
	},
	"custom/weather": {
		"format": "{}",
		"format-alt": "{alt}: {}",
		"format-alt-click": "click-right",
		"interval": 3600,
		"exec": "curl -s 'https://wttr.in/?format=1'",
		"exec-if": "ping wttr.in -c1"
	},
	"custom/pacman": {
		"format": "<big>Û∞áö</big> {}",
		"interval": 3600,
		"exec": "checkupdates-with-aur | wc -l",
		"exec-if": "exit 0",
		"on-click": "footclient -a update yay; pkill -SIGRTMIN+8 waybar",
		"signal": 8,
		"max-length": 8,
		"min-length": 5
	},
	"custom/media": {
		"format": "{icon} {}",
		"return-type": "json",
		"max-length": 40,
		"format-icons": {
			"spotify": "ÔÜº",
			"default": "üéú"
		},
		"escape": true,
		"exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null"
	},
	"custom/power": {
		"format": "<big>Û∞§Ü</big>",
		"tooltip": false,
		// "on-click": "bash ~/.config/fuzzel/scripts/powermenu.sh"
		"on-click": "walker -m power"
	},
	"clock": {
		"format": "ÔÄó  {:%H:%M  ÔÅ≥ %e %b}",
		"tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
		"today-format": "<b>{}</b>"
	},
	"clock#date": {
		"format": "Ôê∫  {:%H:%M  ÔÅ≥ %a, %e %b}",
		"tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
		"today-format": "<b>{}</b>",
		"calendar": {
			"mode": "month",
			"mode-mon-col": 3,
			"weeks-pos": "right",
			"on-scroll": 1,
			"format": {
				"months": "<span color='#ffead3'><b>{}</b></span>",
				"days": "<span color='#C8D2E0'>{}</span>",
				"weeks": "<span color='#FAB387'><b>W{}</b></span>",
				"weekdays": "<span color='#81878F'><b>{}</b></span>",
				"weekends": "<span color='#C26F86'><b>{}</b></span>",
				"today": "<span color='#99ffdd'><b>{}</b></span>"
			}
		},
	},
	"custom/gammastep": {
		"interval": 5,
		"return-type": "json",
		"exec": {
			"pre": "if unit_status=\"$(systemctl --user is-active gammastep)\"; then\nstatus=\"$unit_status ($(journalctl --user -u gammastep.service -g 'Period: ' | tail -1 | cut -d ':' -f6 | xargs))\"\nelse\nstatus=\"$unit_status\"\nfi",
			"alt": "${status:-inactive}",
			"tooltip": "Gammastep is $status"
		},
		"format": "{icon}",
		"format-icons": {
			"activating": "Û∞Å™ ",
			"deactivating": "Û∞Å™ ",
			"inactive": "? ",
			"active (Night)": "ÔÜÜ ",
			"active (Nighttime)": "ÔÜÜ ",
			"active (Transition (Night)": "ÔÜÜ ",
			"active (Transition (Nighttime)": "ÔÜÜ ",
			"active (Day)": "Óåç ",
			"active (Daytime)": "Óåç ",
			"active (Transition (Day)": "Óåç ",
			"active (Transition (Daytime)": "Óåç "
		},
		"on-click": "systemctl --user is-active gammastep && systemctl --user stop gammastep || systemctl --user start gammastep"
	},
	"cpu": {
		"interval": 1,
		"format": "{max_frequency}GHz <span color=\"darkgray\">| {usage}%</span>",
		"max-length": 13,
		"min-length": 13
	},
	"mpd": {
		"max-length": 25,
		"format": "<span foreground='#bb9af7'>ÔÄÅ</span> {title}",
		"format-paused": "ÔÄÅ {title}",
		"format-stopped": "<span foreground='#bb9af7'>ÔÄÅ</span>",
		"format-disconnected": "",
		"on-click": "mpc --quiet toggle",
		"on-click-right": "mpc update; mpc ls | mpc add",
		"on-click-middle": "alacritty -e ncmpcpp",
		"on-scroll-up": "mpc --quiet prev",
		"on-scroll-down": "mpc --quiet next",
		"smooth-scrolling-threshold": 5,
		"tooltip-format": "{title} - {artist} ({elapsedTime:%M:%S}/{totalTime:%H:%M:%S})"
	},
	"custom/title": {
		"format": "{}",
		"interval": 0,
		"return-type": "json",
		"tooltip": false
	},
	"custom/title#name": {
		"format": "{}",
		"interval": 0,
		"return-type": "json",
		"max-length": 35,
		"exec": "$HOME/.scripts/title"
	},
	"hyprland/workspaces": {
		"all-outputs": true,
		"format": "{icon}",
		"format-icons": {
			// "1": "‰∏Ä",
			// "2": "‰∫å",
			// "3": "‰∏â",
			// "4": "Âõõ",
			// "5": "‰∫î",
			// "6": "ÂÖ≠",
			// "7": "‰∏É",
			// "8": "ÂÖ´",
			// "9": "‰πù",
			// "10": "ÂçÅ",
			"special": "ÔÄÖ "
		},
		"on-scroll-up": "hyprctl dispatch workspace e+1 1>/dev/null",
		"on-scroll-down": "hyprctl dispatch workspace e-1 1>/dev/null",
		"sort-by-number": true,
		"active-only": false,
		"show-special": true
	},
	"hyprland/window": {
		"format": "{class}: {title}",
		"icon": true,
		"icon-size": 15,
		"max-length": 100,
		"separate-outputs": true
	},
	"pulseaudio": {
		"scroll-step": 3,
		"format": "{icon} {volume}% {format_source}",
		"format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
		"format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
		"format-muted": "Ôö© {format_source}",
		"format-source": "ÔÑ∞",
		"format-source-muted": "ÔÑ±",
		"format-icons": {
			"headphone": "ÔÄ•",
			"hands-free": "Ôñê",
			"headset": "Ôñê",
			"phone": "ÔÇï",
			"portable": "ÔÇï",
			"car": "ÔÜπ",
			"default": [
				"ÔÄ¶",
				"ÔÄß",
				"ÔÄ®"
			]
		},
		"on-click": "pavucontrol",
		"on-click-right": "amixer sset Master toggle"
	},
	"wireplumber": {
		"on-click": "~/.local/bin/changevol.sh mute",
		"on-click-right": "~/.local/bin/footclient-launch.sh pulsemixer",
		"on-scroll-up": "~/.local/bin/changevol.sh up",
		"on-scroll-down": "~/.local/bin/changevol.sh down",
		"format": "<span foreground='#fab387'>{icon}</span>  {volume}%",
		"format-muted": "Ôë¶  muted",
		"format-source": "ÔÑ∞",
		"format-source-muted": "ÔÑ±",
		"format-icons": {
			"headphone": "ÔÄ• ",
			"hands-free": "Ôñê ",
			"headset": "Ôñê ",
			"phone": "ÔÇï ",
			"portable": "ÔÇï ",
			"car": "ÔÜπ ",
			"default": [
				"ÔÄ¶ ",
				"ÔÄß ",
				"ÔÄ® "
			]
		}
	},
	"keyboard-state": {
		"numlock": false,
		"capslock": true,
		"format": {
			"numlock": "Û∞ΩΩ",
			"capslock": "<big>Û∞™õ</big>"
		},
		"format-icons": {
			"locked": "ÔÄ£",
			"unlocked": "ÔÇú"
		}
	},
	"custom/caps": {
		"format": "{icon}",
		"return-type": "json",
		"format-icons": {
			"lock": "Û∞¨à",
			"unlock": "Û∞Ø´",
		},
		"exec": "bash ~/.config/waybar/scripts/check-caps.sh",
	},
	"custom/notification": {
		"tooltip": false,
		"format": "{icon} ",
		"format-icons": {
			"notification": "ÔÇ¢<span foreground='#f38ba8'><sup>ÔëÑ</sup></span>",
			"none": "ÔÇ¢",
			"dnd-notification": "Ôá∑<span foreground='#f38ba8'><sup>ÔëÑ</sup></span>",
			"dnd-none": "Ôá∑",
			"inhibited-notification": "ÔÇ¢<span foreground='#f38ba8'><sup>ÔëÑ</sup></span>",
			"inhibited-none": "ÔÇ¢",
			"dnd-inhibited-notification": "Ôá∑<span foreground='#f38ba8'><sup>ÔëÑ</sup></span>",
			"dnd-inhibited-none": "Ôá∑"
		},
		"return-type": "json",
		"exec-if": "which swaync-client",
		"exec": "swaync-client -swb",
		"on-click": "swaync-client -t -sw",
		"on-click-right": "swaync-client -d -sw",
		"escape": true
	},
	"tray": {
		"icon-size": 15,
		"spacing": 5
	}
}
